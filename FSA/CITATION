citHeader("To cite package 'FSA' in publications use:")

year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)
citEntry(entry="Manual",
         title = "FSA: Fisheries Stock Analysis",
         author = personList(as.person("Derek H. Ogle")),
         year = year,
         note = note,
         textVersion = paste0("Ogle, D.H. ",year,". FSA: Fisheries Stock Analysis. ",note,".")
)
